{
  "name": "layered-cache",
  "version": "4.0.1",
  "description": "The manager to handle hierarchical cache layers.",
  "main": "src/index.js",
  "scripts": {
    "precommit": "npm run lint",
    "test": "nyc ava --verbose --timeout=10s",
    "test-no-report": "ava --verbose --timeout=10s",
    "fuck": "eslint . --fix",
    "lint": "eslint .",
    "report-cov": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "posttest": "npm run report-cov"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/node-layered-cache.git"
  },
  "keywords": [
    "layered-cache",
    "cache",
    "manager",
    "layer",
    "hierarchical"
  ],
  "engines": {
    "node": ">= 10"
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/node-layered-cache/issues"
  },
  "ava": {
    "babel": false,
    "files": [
      "test/*.js"
    ]
  },
  "devDependencies": {
    "ava": ">= 1.0.0-beta.6",
    "codecov": "^3.0.4",
    "data-source": "^5.3.2",
    "data-source-loader-qq": "^5.2.2",
    "delay": "^3.0.0",
    "eslint": "^5.2.0",
    "eslint-config-ostai": "^1.3.0",
    "eslint-plugin-import": "^2.13.0",
    "lru-cache": "^4.0.2",
    "nyc": "^12.0.2"
  },
  "dependencies": {
    "err-object": "^3.0.5",
    "pending-queue": "^1.2.0",
    "single-batch": "^1.1.3"
  }
}
